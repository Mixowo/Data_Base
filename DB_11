CREATE DATABASE DB_INV_UNIVERSITARIA_

USE DB_INV_UNIVERSITARIA_

CREATE TABLE ARTICULO(
ID_ARTICULO INT PRIMARY KEY NOT NULL,
TITULO_ARTICULO VARCHAR(10),
COPIA_ARTICULO VARCHAR(10),
LUGAR_COPIA VARCHAR(10),
CORREO_CONTACTO VARCHAR(15),
)

CREATE TABLE AUTORES(
ID_AUTOR INT PRIMARY KEY NOT NULL,
NOMBRE_AUTOR VARCHAR(10),
CENTRO_TRABAJO VARCHAR(10),
CORREO VARCHAR(15),
TEMAS VARCHAR(15),
)

CREATE TABLE PALABRAS_CLAVE(
ID_PALABRA_CLAVE INT PRIMARY KEY NOT NULL,
PALABRA VARCHAR(10),
)

CREATE TABLE ARTICULOS_AUTOR(
ID_AUTOR INT,
ID_ARTICULO INT,
FOREIGN KEY (ID_AUTOR) REFERENCES AUTORES,
FOREIGN KEY (ID_ARTICULO) REFERENCES ARTICULO,
)

CREATE TABLE ARTICULOS_PALABRACLAVE(
ID_ARTICULO INT,
ID_PALABRA_CLAVE INT,
FOREIGN KEY (ID_ARTICULO) REFERENCES ARTICULO,
FOREIGN KEY (ID_PALABRA_CLAVE) REFERENCES PALABRAS_CLAVE
)

CREATE TABLE INFORMES_TECNICO(
ID_INFORME_TECNICO INT PRIMARY KEY NOT NULL,
ID_ARTICULO INT,
NUMERO_INFORME INT,
CENTRO_PUBLICACION VARCHAR(10),
MES DATETIME,
Aﾃ前 DATETIME,
FOREIGN KEY (ID_ARTICULO) REFERENCES ARTICULO
)

CREATE TABLE CONGRESO(
ID_CONGRESO INT PRIMARY KEY NOT NULL,
NOMBRE_CONGRESO VARCHAR(10),
TIPO_CONGRESO VARCHAR(10),
FRECUENCIA_CELEBRACION DATETIME,
PAIS VARCHAR(10),
Aﾃ前_PRIMERA_CELEBRACION DATETIME,
)

CREATE TABLE REVISTA_CIENTIFICA(
ID_REVISTA INT PRIMARY KEY NOT NULL,
NOMBRE_REVISTA VARCHAR(10),
NOMBRE_EDITOR VARCHAR(10),
Aﾃ前_PUBLICACION DATETIME,
FRECUENCIA DATETIME,
TEMAS VARCHAR (10),
)

CREATE TABLE ARTICULO_CONGRESO(
ID_CONGRESO INT,
ID_ARTICULO INT,
EDICION_CONGRESO VARCHAR(10),
CIUDAD_CELEBRACION VARCHAR(10),
FECHA_INICIO DATETIME,
FECHA_FINAL DATETIME,
FOREIGN KEY (ID_CONGRESO) REFERENCES CONGRESO,
FOREIGN KEY (ID_ARTICULO) REFERENCES ARTICULO
)

CREATE TABLE ARTICULO_REVISTA(
ID_REVISTA INT,
ID_ARTICULO INT,
NUMERO_REVISTA VARCHAR(10),
PAGINAS_OCUPADAS INT,
Aﾃ前 DATETIME,
FOREIGN KEY (ID_REVISTA) REFERENCES REVISTA_CIENTIFICA,
FOREIGN KEY (ID_ARTICULO) REFERENCES ARTICULO
)
