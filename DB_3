CREATE DATABASE DB_PRESTAMOSLIBROS

USE DB_PRESTAMOSLIBROS

create TABLE AULA(
ID_AULA VARCHAR (10) PRIMARY KEY NOT NULL,
EDIFICIO VARCHAR (15),
PISO VARCHAR (15),
)

CREATE TABLE ASIGNATURA(
ID_ASIGNATURA VARCHAR (10) PRIMARY KEY NOT NULL,
NOMBRE_ASIGNATURA DATETIME,
CODIGO_CURSO VARCHAR (10),
CODIGO_PROFESOR varchar (10),

)

create TABLE PROFESOR(
ID_PROFESOR VARCHAR (10) PRIMARY KEY NOT NULL,
NOMBRE_PROFESOR VARCHAR (20),
CODIGO_ASIGNATURA VARCHAR (10),
FOREIGN KEY (CODIGO_ASIGNATURA) REFERENCES ASIGNATURA
)

create TABLE CURSO(
ID_CURSO VARCHAR (10) PRIMARY KEY NOT NULL,
GRADO VARCHAR (15),
CODIGO_AULA VARCHAR (10),
CODIGO_PROFESOR VARCHAR (10),
FOREIGN KEY (CODIGO_AULA) REFERENCES AULA,
FOREIGN KEY (CODIGO_PROFESOR) REFERENCES PROFESOR,

)

create TABLE LIBRO(
ID_LIBRO VARCHAR (10) PRIMARY KEY NOT NULL,
TIPO VARCHAR (15),
CATEGORIA VARCHAR (20),
EDITORIAL VARCHAR (10),
CODIGO_ASIGNATURA VARCHAR (10)
FOREIGN KEY (CODIGO_ASIGNATURA) REFERENCES ASIGNATURA
)

create TABLE PRESTAMOS(
ID_PRESTAMOS VARCHAR (10) PRIMARY KEY NOT NULL,
FECHA_PRESTAMOS VARCHAR (15),
FECHA_DEVOLUCION VARCHAR (20),
CODIGO_PROFESOR VARCHAR (10),
CODIGO_LIBRO VARCHAR (10),
FOREIGN KEY (CODIGO_PROFESOR) REFERENCES PROFESOR,
FOREIGN KEY (CODIGO_LIBRO) REFERENCES LIBRO,

)
